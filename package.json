{
    "name": "batchencodingconverter",
    "displayName": "BatchEncodingConverter",
    "description": "convert encoding of files in current workspace.",
    "version": "0.0.1",
    "publisher": "yuna611d",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.convertSjisToUTF8"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.convertSjisToUTF8",
                "title": "BatchEncodingConvert: SJIS to UTF8"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
